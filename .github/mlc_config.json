{
    "replacementPatterns": [
        {
            "pattern": "&quot;",
            "replacement": ""
        },
        {
          "pattern": "^/",
          "replacement": "DEPLOYMENT_URL"
        },
        {
            "pattern": "&amp;",
            "replacement": "&"
        }
    ],
    "ignorePatterns": [
        {
          "pattern": "^https*://www.example.com/"
        },
        {
          "pattern": "https://www.mijnwebwinkel.nl/beheer/payment/sorting"
        },
        {
          "pattern": "https://www.shopware.com/en/pricing"
        },
        {
          "pattern": "https://eur-lex.europa.eu/legal-content/EN/TXT/.*"
        },
        {
          "pattern": "https://yourdomain.com/index/paymentprovidernotification.*"
        },
        {
          "pattern": "https://payv2.multisafepay.com/files/{{accountid}}/{{filename}"
        },
        {
          "pattern": "https://www.nationale-tuinbon.nl"
        },
        {
          "pattern": "https://www.shopify.com/login?redirect=%2Fadmin%2Fauthorize_gateway%.*"
        },
        {
          "pattern": "https://yourdomain.webshop.com.*"
        },
        {
          "pattern": "https://support.cloudflare.com/hc/en-us/articles.*"
        },
        {
          "pattern": "https://support.ecwid.com/.*"
        }
      ],
    "timeout": "60s",
    "retryOn429": true,
    "aliveStatusCodes": [200, 206, 999]
}